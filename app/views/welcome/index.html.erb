<div class="welcome_page">
      <script>
                function timercountxx(){

                var x1day = new Date();
                var x1second = Math.floor(Date.UTC(2016, 11 , 25 ,0,0,0)/1000);
                var x1nowsecond = Math.floor(x1day.getTime()/1000);
                var x1timeofset=-540;
                x1nowsecond = x1nowsecond - (x1timeofset*60);
                var x1_nisu = (x1second - x1nowsecond);
                var x1_nisu2 = (x1second - x1nowsecond)/60;
                var x1_nisu3 = (x1second - x1nowsecond)/3600;
                var x1_nisu4 = (x1second - x1nowsecond)/(86400);
                var x1_date = Math.floor(x1_nisu4);
                var x1_hour = Math.floor(x1_nisu3)-(x1_date*24);
                var x1_minite = Math.floor(x1_nisu2)-(((x1_date*24)*60)+(x1_hour*60));
                var x1_second = Math.floor(x1_nisu)-(((((x1_date*24)*60)+(x1_hour*60))*60)+x1_minite*60);
                var x1insertstr = "--<span class=\"days\">あと</span>";
                var x1todaystr = "本日";
                if (x1_date>0){
                    x1insertstr=x1insertstr + x1_date +"<span class=\"days\">日</span>";
                } else {
                    // x1insertstr=x1insertstr + 0 +"<span class=\"days\">日</span>";
                    x1insertstr= x1insertstr + 0 +"<span class=\"days\">日</span>";
                }
                if (x1_hour>0 || x1_nisu>86400){
                    x1insertstr=x1insertstr + x1_hour +"<span class=\"days\">時間</span>";
                } else {
                    x1insertstr=x1insertstr + 0 +"<span class=\"days\">時間</span>";
                }
                if (x1_minite>0 || x1_nisu>3600){
                    x1insertstr=x1insertstr + x1_minite +"<span class=\"days\">分</span>";
                } else {
                    x1insertstr=x1insertstr + 0 +"<span class=\"days\">分</span>";
                }
                x1insertstr=x1insertstr + x1_second + "<span class=\"days\">秒</span>--";
                document.getElementById('counttimerx').innerHTML = x1insertstr;
                }



                function starttimerxx(){
                timercountxx();
                setInterval("timercountxx()",1000);

                }


                var UAUA=0;
                var UA = navigator.userAgent;
                if (UA.indexOf('iPhone') > -1 || UA.indexOf('iPod') > -1 || UA.indexOf('Android') > -1 ) {
                document.write('<link rel="stylesheet" href="./mobile.css">');
                UAUA=1;
                }
    </script>
    <section class="count-content">
            <div class="count-content-inner">
                <div class="count-content-inner-title">
                    <div class="merry-christmas text-center">
                        <h1>2016年クリスマスまで</h1>
                        <h3 id="counttimerx">あと何日？</h3>
                        <script>
                            starttimerxx();
                        </script>
                        <h4><%= "おひとりさまは#{@users.count}人います" %></h4>
                    </div>
                </div>
            </div>
    </section>
  <hr>
  <% unless user_signed_in? && current_user.present? %>
    <div class="row sign_inup_page">
      <div class="col-xl-5 col-lg-5 col-sm-5 col-xs-5 sign_inup_button_area">
        <a href="/users/sign_in" class="sign_inup_button_text">ログインする</a>
      </div>
      <div class="col-xl-2 col-lg-2 col-sm-2 col-xs-2" style="height:200px; background-color:white;">
      </div>
      <div class="col-xl-5 col-lg-5 col-sm-5 col-xs-5 sign_inup_button_area">
        <a href="/users/sign_up" class="sign_inup_button_text">登録する</a>
      </div>
    </div>
  <% else %>
    <% unless current_user.full_profile? %>
        <h3>登録を完了させてください！</h3>
        <%= link_to "登録する", edit_user_path(current_user.id), :class => "btn btn-primary" %>
    <% end %>
  <% end %>
</div>